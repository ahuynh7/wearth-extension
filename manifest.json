{
    "name": "Swear",
    "description": "mvp build for sustainable clothing extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "tabs",
        "scripting",
        "storage"
    ],
    "host_permissions": ["http://*/*", "https://*/*"],
    "background": {
        "service_worker": "src/background.js"
    },
    "icons": {
        "16": "/icons/wearth-16.png",
        "32": "/icons/wearth-32.png",
        "48": "/icons/wearth-64.png",
        "128": "/icons/wearth-128.png"
    }
}